@page "/sindschonferien"

<PageTitle>Weather</PageTitle>


<h1>@Message</h1>


@code {
    public bool Holiday;
    public string Message;

    protected override async Task OnInitializedAsync()
    {
        Holiday = await HolidayProvider.IsHolyDay();
        Message = Holiday ? "Jaaa" : "Nein :(";
    }
}